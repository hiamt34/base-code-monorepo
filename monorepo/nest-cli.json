{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/monorepo/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/monorepo/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/monorepo",
  "projects": {
    "monorepo": {
      "type": "application",
      "root": "apps/monorepo",
      "entryFile": "main",
      "sourceRoot": "apps/monorepo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/monorepo/tsconfig.app.json"
      }
    },
    "backend": {
      "type": "application",
      "root": "apps/backend",
      "entryFile": "main",
      "sourceRoot": "apps/backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/backend/tsconfig.app.json"
      }
    },
    "caching": {
      "type": "application",
      "root": "apps/caching",
      "entryFile": "main",
      "sourceRoot": "apps/caching/src",
      "compilerOptions": {
        "tsConfigPath": "apps/caching/tsconfig.app.json"
      }
    },
    "queueing": {
      "type": "application",
      "root": "apps/queueing",
      "entryFile": "main",
      "sourceRoot": "apps/queueing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/queueing/tsconfig.app.json"
      }
    },
    "mailing": {
      "type": "application",
      "root": "apps/mailing",
      "entryFile": "main",
      "sourceRoot": "apps/mailing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mailing/tsconfig.app.json"
      }
    },
    "sms": {
      "type": "application",
      "root": "apps/sms",
      "entryFile": "main",
      "sourceRoot": "apps/sms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sms/tsconfig.app.json"
      }
    },
    "sendmail": {
      "type": "library",
      "root": "libs/sendmail",
      "entryFile": "index",
      "sourceRoot": "libs/sendmail/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sendmail/tsconfig.lib.json"
      }
    },
    "password-hashing": {
      "type": "library",
      "root": "libs/password-hashing",
      "entryFile": "index",
      "sourceRoot": "libs/password-hashing/src",
      "compilerOptions": {
        "tsConfigPath": "libs/password-hashing/tsconfig.lib.json"
      }
    },
    "decorators/authorization": {
      "type": "library",
      "root": "libs/decorators/authorization",
      "entryFile": "index",
      "sourceRoot": "libs/decorators/authorization/src",
      "compilerOptions": {
        "tsConfigPath": "libs/decorators/authorization/tsconfig.lib.json"
      }
    }
  }
}